					"%%pyspark\n",
					"blob_account_name = \"airportdemoblob\"\n",
					"blob_container_name = \"blobairportdata\"\n",
					"from pyspark.sql import SparkSession\n",
					"\n",
					"sc = SparkSession.builder.getOrCreate()\n",
					"token_library = sc._jvm.com.microsoft.azure.synapse.tokenlibrary.TokenLibrary\n",
					"blob_sas_token = token_library.getConnectionString(\"airportdemoblob\")\n",
					"\n",
					"spark.conf.set(\n",
					"    'fs.azure.sas.%s.%s.blob.core.windows.net' % (blob_container_name, blob_account_name),\n",
					"    blob_sas_token)\n",
					"df = spark.read.load('wasbs://blobairportdata@airportdemoblob.blob.core.windows.net/yellow_tripdata_2021-01.parquet', format='parquet')\n",
